<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.news.NewsFragment"
    android:background="@color/light_pink">

    <TextView
        android:id="@+id/news_info"
        android:layout_width="187dp"
        android:layout_height="150dp"
        android:layout_marginTop="144dp"
        android:fontFamily="@font/bakso_sapi"
        android:text="fra 5 august til 9 august er vi på"
        android:textAlignment="center"
        android:textColor="@color/redpink"
        android:textSize="@dimen/medium_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/news_info2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:fontFamily="@font/bakso_sapi"
        android:text="øya festivalen"
        android:textColor="@color/blue"
        android:textSize="@dimen/medium_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/news_info" />


    <ImageView
        android:id="@+id/back_button_news"
        android:layout_width="110dp"
        android:layout_height="52dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contact_info_textView2"
        app:layout_constraintVertical_bias="0.96"
        tools:srcCompat="@drawable/back_arrow" />


</androidx.constraintlayout.widget.ConstraintLayout>